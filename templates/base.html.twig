{% set user_hash = app.request.cookies.get('auth_hash') %}
{% set city = city_term|split(',') %}
{% set meta = {
    'main': {
        'slug': '',
        'item_menu': 'Главная',
        'title': 'Индивидуалки и проститутки ' ~ city[1],
        'description': 'Проститутки ' ~ city[1] ~ ' от недорогих до экстра-класса. Самые лучшие индивидуалки ' ~ city[1] ~ ' работают для мужчин круглосуточно. Отдых для взрослых.',
        'keywords': 'досуг, проститутки, индивидуалки, мужской эскорт, трансексуалы, сауна, гостиница, квартира на час, ' ~ city[0]|lower ~ ', недорого, дешевые, фото'
    },
    'ind': {
        'slug': 'individualki',
        'item_menu': 'Индивидуалки ',
        'title': 'Индивидуалки ' ~ city[1],
        'description': 'Индивидуалки и дешевые проститутки ' ~ city[1] ~ '. Недорогие девочки по вызову ' ~ city[2] ~ ' для состоятельных мужчин или элитные проститутки для комфортного досуга.'  ~ city[2],
        'keywords': 'проститутки, индивидуалки, ' ~ city[0]|lower ~ ', элитные, досуг, vip, недорогие, фото',
        'one_name': 'Индивидуалка'
    },
    'sal': {
        'slug': 'salony',
        'item_menu': 'Салоны',
        'title': 'Интимсалоны ' ~ city[1],
        'description': 'Интимсалоны ' ~ city[1] ~ '. Недорогие индивидуалки ' ~ city[1] ~ ' ждут Вас в своих апартаментах круглосуточно. Самые лучшие девочки по вызову.',
        'keywords': 'вызвать, проститутки, салон, ' ~ city[0]|lower ~ ', элитные, досуг, vip, недорогие, фото',
        'one_name': 'Салон'
    },
    'man': {
        'slug': 'muzhskoi-eskort',
        'item_menu': 'Мужской эскорт',
        'title': 'Мужской эскорт '  ~ city[1],
        'description': 'Мужчины по вызову ' ~ city[2] ~ '. Мужской эксорт как и индивидуалки ' ~ city[1] ~ ' только для самых лучших и состоятельных женщин.',
        'keywords': 'вызвать, мужчины, проститутки, ' ~ city[0]|lower ~ ', элитные, досуг, vip, недорогие, фото',
        'one_name': 'Мужчина по вызову'
    },
    'tsl': {
        'slug': 'transseksualki',
        'item_menu': 'Трансы',
        'title': 'Трансексуалки ' ~ city[1],
        'description': 'Трансексуалки ' ~ city[1] ~ '. Для ценителей экзотики в досуге к вашему вниманию трансексуалы ' ~ city[2] ~ '.',
        'keywords': 'вызвать, трансексуалы, проститутки, ' ~ city[0]|lower ~ ', элитные, досуг, vip, недорогие, фото',
        'one_name': 'Транссексуалка'
    }
} %}
<!DOCTYPE html>
<html data-bs-theme="dark" lang="ru-RU">
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta charset="UTF-8">
        <title>{% block title %}{% endblock %} | {{ site_name }}</title>
        <meta name="description" content="{% block description %}{% endblock %}">
        <link rel="shortcut  icon" type="image/x-icon" href="data:image/x-icon;base64,">
        <link rel="icon" href="{{ asset('images/fav.ico') }}" type="image/x-icon">
        {% block stylesheets %}
            <link rel="preconnect" href="https://fonts.googleapis.com">
            <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
            <link href="https://fonts.googleapis.com/css2?family=Cuprum&family=Great+Vibes&family=Roboto+Slab:wght@700&display=swap" rel="stylesheet">
        {% endblock %}
        {% block javascripts %}{% block importmap %}{{ importmap('app') }}{% endblock %}{% endblock %}
    </head>
    <body class="bg-body-tertiary">
        <section class="content">
            {% include 'widget/menu.html.twig' with {menu: meta, user_hash} %}
            {% block body %}{% endblock %}
        </section>
        {% include 'widget/footer.html.twig' %}
    </body>
</html>
